    "@delon/form": "^9.2.3",
    "@delon/theme": "^9.2.3",
    "@delon/util": "^9.2.3",
    "@ngx-translate/core": "^12.1.2",
    "ajv": "^6.12.0",
    "html2canvas": "^1.0.0-rc.5",
    "jspdf": "^1.5.3",
